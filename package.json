  "devDependencies": {
    "@types/node": "^16.11.0",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "class-validator": "^0.13.1",
    "graphql": "^15.6.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
